{
    "name": "vscode-gitweblinks",
    "displayName": "Git Web Links for VS Code",
    "description": "Copy links to files in their online Git repositories",
    "version": "1.2.1",
    "publisher": "reduckted",
    "homepage": "https://github.com/reduckted/vscode-gitweblinks",
    "repository": {
        "type": "git",
        "url": "https://github.com/reduckted/vscode-gitweblinks"
    },
    "bugs": {
        "url": "https://github.com/reduckted/vscode-gitweblinks/issues"
    },
    "icon": "images/icon.png",
    "galleryBanner": {
        "color": "#F0F0E6",
        "theme": "light"
    },
    "keywords": [
        "git",
        "github",
        "bitbucket",
        "multi-root ready"
    ],
    "private": true,
    "main": "./out/src/extension",
    "license": "MIT",
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test",
        "lint": "tslint -p tsconfig.json"
    },
    "dependencies": {
        "copy-paste": "^1.3.0",
        "spawn-rx": "^2.0.12"
    },
    "devDependencies": {
        "@types/chai": "^4.0.4",
        "@types/copy-paste": "^1.1.30",
        "@types/mkdirp": "^0.5.1",
        "@types/mocha": "^2.2.44",
        "@types/node": "^8.0.51",
        "@types/rimraf": "^2.0.2",
        "@types/sinon": "^2.3.7",
        "@types/sinon-chai": "^2.7.29",
        "@types/uuid": "^3.4.3",
        "chai": "^4.1.2",
        "mkdirp": "^0.5.1",
        "mocha": "^4.0.1",
        "rimraf": "^2.6.2",
        "sinon": "^4.1.2",
        "sinon-chai": "^2.14.0",
        "tslint": "^5.8.0",
        "typescript": "^2.6.1",
        "uuid": "^3.1.0",
        "vscode": "^1.1.6"
    },
    "engines": {
        "vscode": "^1.18.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "contributes": {
        "commands": [
            {
                "command": "gitweblinks.copyFile",
                "title": "Copy Web Link to File"
            },
            {
                "command": "gitweblinks.copySelection",
                "title": "Copy Web Link to Selection"
            }
        ],
        "menus": {
            "editor/context": [
                {
                    "command": "gitweblinks.copySelection",
                    "group": "gitweblinks@1",
                    "when": "gitweblinks:canCopy"
                }
            ],
            "editor/title/context": [
                {
                    "command": "gitweblinks.copyFile",
                    "group": "gitweblinks@1",
                    "when": "gitweblinks:canCopy"
                }
            ],
            "explorer/context": [
                {
                    "command": "gitweblinks.copyFile",
                    "group": "gitweblinks@1",
                    "when": "gitweblinks:canCopy"
                }
            ]
        },
        "configuration": {
            "title": "Git Web Links configuration",
            "properties": {
                "gitweblinks.linkType": {
                    "type": "string",
                    "description": "The type of links to create:\n - \"hash\": Create a link to the current commit (default).\n - \"branch\": Create a link to the current branch.",
                    "enum": [
                        "hash",
                        "branch"
                    ],
                    "default": "hash"
                },
                "gitweblinks.bitbucketServer": {
                    "type": "array",
                    "description": "The URLs of your Bitbucket Server instances.",
                    "items": {
                        "type": "object",
                        "properties": {
                            "baseUrl": {
                                "type": "string",
                                "description": "The base URL for the website."
                            },
                            "sshUrl": {
                                "type": "string",
                                "description": "The SSH URL for remotes."
                            }
                        },
                        "required": [
                            "baseUrl"
                        ]
                    }
                },
                "gitweblinks.gitHubEnterprise": {
                    "type": "array",
                    "description": "The URLs of your GitHub Enterprise instances.",
                    "items": {
                        "type": "object",
                        "properties": {
                            "baseUrl": {
                                "type": "string",
                                "description": "The base URL for the website."
                            },
                            "sshUrl": {
                                "type": "string",
                                "description": "The SSH URL for remotes."
                            }
                        },
                        "required": [
                            "baseUrl"
                        ]
                    }
                }
            }
        }
    }
}
