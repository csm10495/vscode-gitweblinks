{
    "name": "vscode-gitweblinks",
    "displayName": "Git Web Links for VS Code",
    "description": "Copy links to files in their online Git repositories",
    "version": "1.4.0",
    "publisher": "reduckted",
    "homepage": "https://github.com/reduckted/vscode-gitweblinks",
    "repository": {
        "type": "git",
        "url": "https://github.com/reduckted/vscode-gitweblinks"
    },
    "bugs": {
        "url": "https://github.com/reduckted/vscode-gitweblinks/issues"
    },
    "icon": "images/icon.png",
    "galleryBanner": {
        "color": "#F0F0E6",
        "theme": "light"
    },
    "keywords": [
        "git",
        "github",
        "bitbucket",
        "multi-root ready"
    ],
    "private": true,
    "main": "./dist/extension",
    "license": "MIT",
    "scripts": {
        "vscode:prepublish": "webpack --mode production",
        "webpack": "webpack --mode development",
        "webpack-dev": "webpack --mode development --watch",
        "test-compile": "tsc -p ./",
        "lint": "tslint -p tsconfig.json"
    },
    "dependencies": {
        "spawn-rx": "2.0.12"
    },
    "devDependencies": {
        "@types/chai": "4.1.7",
        "@types/mkdirp": "0.5.2",
        "@types/mocha": "5.2.7",
        "@types/node": "12.0.7",
        "@types/rimraf": "2.0.2",
        "@types/sinon": "7.0.12",
        "@types/sinon-chai": "3.2.2",
        "@types/uuid": "3.4.4",
        "chai": "4.2.0",
        "mkdirp": "0.5.1",
        "mocha": "6.1.4",
        "prettier": "1.18.2",
        "rimraf": "2.6.3",
        "sinon": "7.3.2",
        "sinon-chai": "3.3.0",
        "ts-loader": "6.0.4",
        "tslint": "5.17.0",
        "typescript": "3.5.1",
        "uuid": "3.3.2",
        "vscode": "1.1.36",
        "webpack": "4.35.0",
        "webpack-cli": "3.3.5"
    },
    "engines": {
        "vscode": "^1.30.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "contributes": {
        "commands": [
            {
                "command": "gitweblinks.copyFile",
                "title": "Copy Web Link to File"
            },
            {
                "command": "gitweblinks.copySelection",
                "title": "Copy Web Link to Selection"
            }
        ],
        "menus": {
            "editor/context": [
                {
                    "command": "gitweblinks.copySelection",
                    "group": "gitweblinks@1",
                    "when": "gitweblinks:canCopy"
                }
            ],
            "editor/title/context": [
                {
                    "command": "gitweblinks.copyFile",
                    "group": "gitweblinks@1",
                    "when": "gitweblinks:canCopy"
                }
            ],
            "explorer/context": [
                {
                    "command": "gitweblinks.copyFile",
                    "group": "gitweblinks@1",
                    "when": "gitweblinks:canCopy"
                }
            ]
        },
        "configuration": {
            "title": "Git Web Links configuration",
            "properties": {
                "gitweblinks.linkType": {
                    "type": "string",
                    "description": "The type of links to create:\n - \"hash\": Create a link to the current commit (default).\n - \"branch\": Create a link to the current branch.",
                    "enum": [
                        "hash",
                        "branch"
                    ],
                    "default": "hash"
                },
                "gitweblinks.bitbucketServer": {
                    "type": "array",
                    "description": "The URLs of your Bitbucket Server instances.",
                    "items": {
                        "type": "object",
                        "properties": {
                            "baseUrl": {
                                "type": "string",
                                "description": "The base URL for the website."
                            },
                            "sshUrl": {
                                "type": "string",
                                "description": "The SSH URL for remotes."
                            }
                        },
                        "required": [
                            "baseUrl"
                        ]
                    }
                },
                "gitweblinks.gitHubEnterprise": {
                    "type": "array",
                    "description": "The URLs of your GitHub Enterprise instances.",
                    "items": {
                        "type": "object",
                        "properties": {
                            "baseUrl": {
                                "type": "string",
                                "description": "The base URL for the website."
                            },
                            "sshUrl": {
                                "type": "string",
                                "description": "The SSH URL for remotes."
                            }
                        },
                        "required": [
                            "baseUrl"
                        ]
                    }
                }
            }
        }
    }
}
